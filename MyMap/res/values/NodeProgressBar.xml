<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NodeProgressBar">
        <attr name="baseColor" format="reference|color" />
        <attr name="progressColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="progressWidth" format="dimension"></attr>
        <attr name="nodeRadius" format="dimension"></attr>
        <attr name="textSize" format="dimension" />
        <attr name="maxNode" format="integer"></attr>
        <attr name="nodesInfo" format="reference"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

</resources>